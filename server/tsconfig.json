{
  "compilerOptions": {
    "outDir": "lib",
    "rootDir": "",
    "module": "commonJs",
    "lib": ["es6"],
    "target": "es2016",
    "declaration": true,
    "removeComments": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
